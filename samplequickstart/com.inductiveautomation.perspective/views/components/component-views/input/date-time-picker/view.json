                "type": "ia.display.label"
              },
              {
                "meta": {
                  "name": "Value"
                },
                "position": {
                  "basis": "32px",
                  "shrink": 0
                },
                "propConfig": {
                  "props.text": {
                    "binding": {
                      "config": {
                        "path": "../DateTimePicker.props.formattedValue"
                      },
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "style": {
                    "textAlign": "center"
                  }
                },
                "type": "ia.display.label"
              },
              {
                "meta": {
                  "name": "DateTimePicker"
                },
                "position": {
                  "shrink": 0
                },
                "props": {
                  "format": "YYYY-MM-DD HH:mm:ss ZZ"
                },
                "type": "ia.input.date-time-picker"
              }
