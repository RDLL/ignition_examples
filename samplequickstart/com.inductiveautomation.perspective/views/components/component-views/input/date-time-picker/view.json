            "meta": {
              "name": "DateTime"
            },
            "position": {
              "basis": "300px",
              "shrink": 0
            },
            "props": {
              "alignContent": "flex-start",
              "direction": "column"
            },
            "type": "ia.container.flex"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "Label"
                },
                "position": {
                  "basis": "50px",
                  "shrink": 0
                },
                "props": {
                  "style": {
                    "textAlign": "center"
                  },
                  "text": "Limited to within 7 days:"
                },
                "type": "ia.display.label"
              },
              {
                "meta": {
                  "name": "Value"
                },
                "position": {
                  "basis": "32px",
                  "shrink": 0
                },
                "propConfig": {
                  "props.text": {
                    "binding": {
                      "config": {
                        "path": "../DateTimePicker.props.formattedValue"
                      },
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "style": {
                    "textAlign": "center"
                  }
                },
                "type": "ia.display.label"
              },
              {
                "meta": {
                  "name": "DateTimePicker"
                },
                "position": {
                  "shrink": 0
                },
                "propConfig": {
                  "props.maxDate": {
                    "binding": {
                      "config": {
                        "expression": "addDays(now(0), 7)"
                      },
                      "type": "expr"
                    }
                  },
                  "props.minDate": {
                    "binding": {
                      "config": {
                        "expression": "addDays(now(0), -7)"
                      },
                      "type": "expr"
                    }
                  }
                },
                "type": "ia.input.date-time-picker"
              }
            ],
            "meta": {
              "name": "Limited"
