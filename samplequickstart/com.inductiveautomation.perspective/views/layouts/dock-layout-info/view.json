{
  "custom": {},
  "params": {
    "desc": "Layout DEETS here.",
    "title": "Layout Title"
  },
  "propConfig": {
    "params.desc": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.title": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "width": 400
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "title"
        },
        "position": {
          "basis": "32px",
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.title"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "framework/page-title",
            "color": "var(--neutral-10)",
            "paddingRight": "8px"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "desc"
        },
        "position": {
          "basis": "200px",
          "grow": 1
        },
        "propConfig": {
          "props.source": {
            "binding": {
              "config": {
                "path": "view.params.desc"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "color": "var(--neutral-10)",
            "marginTop": "8px",
            "paddingLeft": "8px",
            "paddingRight": "8px"
          }
        },
        "type": "ia.display.markdown"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "backgroundColor": "var(--neutral-90)"
      }
    },
    "type": "ia.container.flex"
  }
}