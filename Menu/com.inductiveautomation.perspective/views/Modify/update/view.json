{
  "custom": {},
  "params": {},
  "props": {},
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "basis": "136px"
            },
            "props": {
              "style": {
                "background-color": "blue",
                "color": "white",
                "margin": "5px",
                "padding": "10px"
              },
              "text": "Actual Address"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "TextField"
            },
            "position": {
              "basis": "150px",
              "grow": 1
            },
            "props": {
              "style": {
                "margin": "5px"
              }
            },
            "type": "ia.input.text-field"
          }
        ],
        "meta": {
          "name": "actualAddress"
        },
        "position": {
          "basis": "50px"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "basis": "135px"
            },
            "props": {
              "style": {
                "background-color": "green",
                "color": "white",
                "margin": "5px",
                "padding": "10px"
              },
              "text": "New Address"
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "TextField"
            },
            "position": {
              "basis": "150px",
              "grow": 1
            },
            "props": {
              "style": {
                "margin": "5px"
              }
            },
            "type": "ia.input.text-field"
          }
        ],
        "meta": {
          "name": "newAddress"
        },
        "position": {
          "basis": "50px"
        },
        "type": "ia.container.flex"
      },
      {
        "meta": {
          "name": "Table"
        },
        "position": {
          "basis": "400px"
        },
        "propConfig": {
          "props.data": {
            "binding": {
              "config": {
                "parameters": {
                  "address": "{../actualAddress/TextField.props.text}"
                },
                "polling": {
                  "enabled": true,
                  "rate": "5"
                },
                "queryPath": "getAddress"
              },
              "type": "query"
            }
          }
        },
        "type": "ia.display.table"
      },
      {
        "meta": {
          "name": "Button"
        },
        "position": {
          "basis": "34px"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "parameters": {
                  "newAddress": "{../newAddress/TextField.props.text}",
                  "oldAddress": "{../actualAddress/TextField.props.text}"
                },
                "queryPath": "updateAddress"
              },
              "type": "query"
            }
          }
        },
        "type": "ia.input.button"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}